{
    "project": {
        "name": "simple_chat_agent",
        "description": "A simple chat agent using LangGraph"
    },
    "dependencies": {
        "python": "3.10",
        "requirements": [
            "langchain>=0.0.350",
            "openai>=1.0.0",
            "langgraph>=0.0.10",
            "fastapi",
            "uvicorn",
            "python-dotenv"
        ]
    },
    "graphs": {
        "chat": {
            "module": "main",
            "graph": "graph",
            "description": "A simple chat agent that can engage in conversations",
            "config": {
                "type": "state_graph",
                "entry_point": "main:graph"
            }
        }
    }
}
